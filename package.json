{
  "name": "next-version",
  "version": "0.0.0",
  "private": true,
  "description": "Next version GitHub Action using conventional commits",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conventional-actions/next-version.git"
  },
  "homepage": "https://github.com/conventional-actions/next-version",
  "bugs": {
    "url": "https://github.com/conventional-actions/next-version/issues"
  },
  "keywords": [
    "GitHub",
    "action",
    "version",
    "versions",
    "generation",
    "generate",
    "bump",
    "prepare"
  ],
  "author": "Conventional Actions",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "concat-stream": "^2.0.0",
    "conventional-commits-filter": "^2.0.7",
    "conventional-commits-parser": "^3.2.4",
    "git-raw-commits": "^2.0.11",
    "git-semver-tags": "^4.1.1",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@types/concat-stream": "^2.0.0",
    "@types/conventional-commits-parser": "^3.0.2",
    "@types/git-raw-commits": "^2.0.1",
    "@types/git-semver-tags": "^4.1.1",
    "@types/node": "^18.11.4",
    "@types/semver": "^7.3.12",
    "@typescript-eslint/parser": "^5.40.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.25.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.2.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
